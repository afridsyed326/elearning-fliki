{
  "name": "elearning-fliki",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "format": "prettier \"{apps,libs}/**/*.{ts,tsx,js,json}\" --ignore-path .gitignore",
    "format:check": "pnpm format --check",
    "format:write": "pnpm format --write",
    "build": "pnpm exec nx run-many --target build",
    "tsc": "pnpm exec nx run-many --target tsc",
    "lint": "pnpm exec nx run-many --target lint",
    "validate": "pnpm format:write && pnpm tsc && pnpm lint && pnpm build",
    "prepare": "husky install",
    "qc": "git add . && git commit -m"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.2",
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^16.1.2",
    "nx": "21.2.1",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "typescript": "^5.8.3"
  }
}
